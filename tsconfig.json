{
  "compilerOptions": {
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "module": "commonjs",
    "declaration": true,
    "removeComments": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "target": "es6",
    "sourceMap": true,
    "outDir": "./dist",
    "baseUrl": "./",
    "paths": {
      "@listStatus":["src/listStatus"],
      "@interfaces/*":["src/interfaces/*"],
    },   
    "incremental": true
  },
  "exclude": ["node_modules", "dist"]
}
